{
  "name": "service-jobs",
  "version": "1.0.0",
  "description": "Service for image-scanner app. Manages jobs and scans for files.",
  "main": "src/service.ts",
  "scripts": {
    "proto:gen": "bash proto-gen.sh",
    "keys:gen": "openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout ./certs/server.key -out ./certs/server.crt",
    "start": "ts-node service",
    "testdb": "ts-node src/test-db.ts",
    "dev": "SERVICE_NAME='JobManager' RABBIT_MQ_SERVICE_QUEUE_NAME='JobManager' nodemon src/service.ts",
    "build": "tsc",
    "test": "mocha --require ts-node/register tests/**/*.spec.ts --exit",
    "test2": "mocha --require ts-node/register tests/RabbitMq.spec.ts --exit"
  },
  "keywords": [],
  "author": "leonbaird@mac.com",
  "license": "ISC",
  "devDependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@types/amqplib": "^0.10.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/imagemagick": "^0.0.35",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.30",
    "@types/node-jose": "^1.1.13",
    "@types/supertest": "^6.0.3",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "grpc-tools": "^1.13.0",
    "mocha": "^11.5.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "protobufjs": "^7.5.3",
    "protoc-gen-js": "3.21.4-4",
    "protoc-gen-ts": "^0.8.7",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "6.9.0",
    "amqplib": "^0.10.8",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "imagemagick": "^0.1.3",
    "jose-node-cjs-runtime": "^5.10.0",
    "readdir-enhanced": "^6.0.4",
